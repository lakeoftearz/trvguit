<%= form_for ([@review.fcomp, @review])  do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      
            <%= f.label :fullname %>
      <%= f.text_field :fullname %>
      <%= f.label :country %>
      <%= f.text_field :country %>
         <%= f.label :band %>
      <%= f.text_field :band %>
      

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :content %>
      <%= f.text_area :content %>
           

      <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
