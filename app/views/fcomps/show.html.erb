<% provide(:title, @fcomp.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        
        <%= @fcomp.name %>
        <br>
        <%= @fcomp.country %>
        <br>
        <%= @fcomp.rules %>
        <br>
        <%= @fcomp.webpage %>
        <br>
        <%= image_tag @fcomp.logo.url unless @fcomp.logo.blank? %> 
      </h1>
    </section>
  </aside>
</div>